<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>compruebaDatos</servlet-name>
        <servlet-class>compruebaDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compruebaDinero</servlet-name>
        <servlet-class>compruebaDinero</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compruebaDineroTotal</servlet-name>
        <servlet-class>compruebaDineroTotal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compruebaPelisTotal</servlet-name>
        <servlet-class>compruebaPelisTotal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compruebaActores</servlet-name>
        <servlet-class>compruebaActores</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>compruebaDatos</servlet-name>
        <url-pattern>/compruebaDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compruebaDinero</servlet-name>
        <url-pattern>/compruebaDinero</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compruebaDineroTotal</servlet-name>
        <url-pattern>/compruebaDineroTotal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compruebaPelisTotal</servlet-name>
        <url-pattern>/compruebaPelisTotal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compruebaActores</servlet-name>
        <url-pattern>/compruebaActores</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
